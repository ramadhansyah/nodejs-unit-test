{
  "name": "nodejs-tdd",
  "version": "1.0.0",
  "description": "nodejs unit test development",
  "main": "app.js",
  "scripts": {
    "start": "node app",
    "test": "jest --watchAll"
  },
  "author": "adanSource",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "eslint": "^8.28.0",
    "jest": "^29.3.1",
    "supertest": "^6.3.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "extends":[
      "eslint:recommended"
    ],
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "semi":"warn",
      "quotes":["warn","single"]
    }
  }
}
